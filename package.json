{
  "name": "middleman-grunt-boilerplate",
  "version": "1.0.0",
  "description": "This is a static site generated by Middleman with asset compilation by Grunt",
  "main": "Gruntfile.js",
  "keywords": [],
  "homepage": "https://github.com/digital-telepathy/middleman-grunt-boilerplate#readme",
  "author": "Digital Telepathy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-telepathy/middleman-grunt-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/digital-telepathy/middleman-grunt-boilerplate/issues"
  },
  "engines": {
    "node": "~5.12.0",
    "npm": "~3.8.6"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "bash postnpminstall.sh",
    "start": "concurrently --kill-others --raw 'bundle exec middleman' 'grunt' 'node_modules/http-server/bin/http-server ./www -p 5678 --cors'",
    "build": "bundle exec middleman build",
    "deploy": "npm run build && gh-pages -d www",
    "deploy:heroku": "git push heroku master"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "concurrently": "^3.1.0",
    "gh-pages": "^0.11.0",
    "grunt": "^1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cache-bust": "^1.3.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-postcss": "^0.8.0",
    "grunt-sass": "^1.2.0",
    "http-server": "^0.9.0",
    "load-grunt-tasks": "^3.5.2",
    "postcss-flexbugs-fixes": "^2.0.0"
  }
}
